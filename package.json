    "husky": "0.14.3",
    "jest": "23.6.0",
    "jest-emotion": "9.2.7",
    "prettier": "1.14.2",
    "pretty-quick": "1.6.0",
    "react-hot-loader": "4.3.8",
    "react-testing-library": "5.0.1",
    "rimraf": "2.6.2",
    "ts-jest": "23.1.4",
    "ts-loader": "5.1.0",
    "tslint": "5.11.0",
    "tslint-config-prettier": "1.15.0",
