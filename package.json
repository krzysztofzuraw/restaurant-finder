  "private": false,
  "scripts": {
    "dev": "webpack-dev-server --mode development --config configs/webpack.config.dev.js --hot",
    "build": "webpack --mode production --config configs/webpack.config.prod.js",
    "tsc": "tsc --project . --noEmit",
    "test": "jest --config jest.config.js",
    "test:ci": "jest --config jest.config.js && codecov",
    "lint": "tslint --project .",
    "prelint": "pretty-quick",
    "precommit": "yarn run lint",
    "prepush": "concurrently 'yarn run lint' 'yarn run tsc' 'yarn run test'",
    "reinstall": "rimraf node_modules && yarn install",
    "clean:dist": "rimraf dist"
  },
  "devDependencies": {
    "@types/jest": "23.3.2",
    "@types/mapbox-gl": "0.48.0",
